# StringVentory - Environment Configuration

# Copy this file to .env.local and update with your values

# API Configuration
VITE_API_BASE_URL=https://stringventory.onrender.com/v1/
VITE_API_TIMEOUT=30000

# Application Settings
VITE_APP_NAME=StringVentory
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MESSAGING=true
VITE_ENABLE_REPORTS=true

# Authentication
VITE_AUTH_TOKEN_KEY=stringventory_access_token
VITE_AUTH_REFRESH_TOKEN_KEY=stringventory_refresh_token
VITE_AUTH_USER_KEY=stringventory_user
VITE_TOKEN_EXPIRY_BUFFER=300

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# External Services (Optional)
VITE_SENTRY_DSN=
VITE_ANALYTICS_ID=

# API Rate Limiting
VITE_API_RATE_LIMIT=100
VITE_API_RATE_LIMIT_WINDOW=60000

# Feature Gating
VITE_ENABLE_MULTITENANCY=true
VITE_ENABLE_FEATURE_GATING=true
